<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivityProfilePageSix"
    android:background="@drawable/background">
    <LinearLayout
        android:layout_width="91dp"
        android:layout_height="28dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:layout_width="15dp"
            android:layout_height="23sp"
            android:layout_marginTop="3sp"
            android:background="@drawable/icon"
            android:onClick="onClickGoMain" />

        <TextView
            android:layout_width="66dp"
            android:layout_height="28dp"
            android:layout_marginStart="7dp"
            android:text="@string/back_page"
            android:textColor="@color/white"
            android:textSize="22sp" />


    </LinearLayout>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="385dp"
        android:layout_height="105dp"
        android:text="@string/this_seed_phrase_is_the_master_key_to_access_your_wallet_profile_page_six"
        android:textColor="@color/white"
        android:textSize="29sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.22" />

    <TextView

        android:id="@+id/textView5"
        android:layout_width="385dp"
        android:layout_height="69dp"
        android:layout_marginBottom="400dp"
        android:text="@string/check_all_the_boxes_to_confirm_that_you_understand_the_importance_of_your_seed_phrase__profile_page_six"
        android:textColor="@color/white"
        android:textSize="19sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:layout_width="32sp"
        android:layout_height="32sp"
        android:background="@drawable/tick"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView6"
        app:layout_constraintHorizontal_bias="0.551"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.095" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="32sp"
        android:layout_height="32sp"
        android:background="@drawable/tick"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView6"
        app:layout_constraintHorizontal_bias="0.551"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.35" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="32sp"
        android:layout_height="32sp"
        android:background="@drawable/tick"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView6"
        app:layout_constraintHorizontal_bias="0.551"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.616"
        android:importantForAccessibility="no" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="350dp"
        android:layout_height="46dp"
        android:text="@string/write_down_your_seed_phrase_and_store_it_in_a_safe_place_offline__profile_page_six"
        android:textColor="@color/white"
        android:textSize="19sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.601" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="350dp"
        android:layout_height="92dp"
        android:text="@string/it_is_not_recommended_to_save_the_password_digitally_in_plain_text_for_example_in_the_format_of_a_screenshot_password_or_send_the_password_by_e_mail__profile_page_six"
        android:textColor="@color/white"
        android:textSize="19sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.331" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="350dp"
        android:layout_height="46dp"
        android:text="@string/wallet_does_not_store_a_copy_of_your_keyword__profile_page_six"
        android:textColor="@color/white"
        android:textSize="19sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.098" />

</androidx.constraintlayout.widget.ConstraintLayout>